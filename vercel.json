{
    "version": 2,
    "buildCommand": "echo 'No build needed'",
    "outputDirectory": ".",
    "redirects": [
        {
            "source": "/viiibe",
            "destination": "/viiibe/",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/success.html",
            "destination": "/landing/success.html"
        },
        {
            "source": "/cancel.html",
            "destination": "/landing/cancel.html"
        },
        {
            "source": "/reset-tool.html",
            "destination": "/landing/reset-tool.html"
        },
        {
            "source": "/viiibe/",
            "destination": "/landing/index.html"
        },
        {
            "source": "/viiibe/(.*)",
            "destination": "/landing/$1"
        }
    ],
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-store, max-age=0"
                }
            ]
        }
    ]
}